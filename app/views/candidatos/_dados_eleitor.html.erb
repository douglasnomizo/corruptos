<% end %>
<%= ele.input :nome, :required => false, :disabled => true %>
<%= ele.input :nome_mae, :required => false, :disabled => true %>
<%= ele.input :rg, :required => false, :disabled => true %>
<%= ele.input :data_nascimento, :as => :string, :required => false, :disabled => true %>
