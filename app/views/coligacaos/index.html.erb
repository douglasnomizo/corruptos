<h1>Lista de coligacaos</h1>

<table>
  <tr>
    <th>Cargo eleicao</th>
    <th>Nome</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @coligacaos.each do |coligacao| %>
  <tr>
    <td><%= coligacao.cargo_eleicao %></td>
    <td><%= coligacao.nome %></td>
    <td><%= link_to content_tag(:span, "Exibir"), coligacao %></td>
    <td><%= link_to content_tag(:span, "Alterar"), edit_coligacao_path(coligacao) %></td>
    <td><%= link_to content_tag(:span, "Excluir"), coligacao, method: :delete, data: { confirm: 'Tem certeza?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nova Coligacao', new_coligacao_path %>
