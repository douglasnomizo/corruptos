<% if @header %>
	<div class="page-header"><h1><small><%= @header %></small></h1></div>
<% end %>

<table class="table table-condensed table-striped">

	<% if @votos.size > 0 %>
	  <tr>
	  	<th>Votos</th>
	    <th>Zona</th>
	    <th>Seção</th>
			<th>Município</th>
	    <th>Estado</th>
	  </tr>
  <% end %>

	<% @votos.each do |votos_urna| %>
	  <tr>
			<td><%= votos_urna.qtd_votos %></td>
			<td><%= votos_urna.secao.zona.descricao %></td>
	    <td><%= votos_urna.secao.codigo %></td>
	    <td><%= votos_urna.secao.zona.endereco.municipio.nome %></td>
	    <td><%= votos_urna.secao.zona.endereco.municipio.uf.nome %></td>
	  </tr>
	<% end %>
</table>

<br />
Total de Votos: <%= @total_votos %>