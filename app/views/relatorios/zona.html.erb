<% if @header %>
	<div class="page-header"><h1><small><%= @header %></small></h1></div>
<% end %>

<% if @secoes.size > 0 %>

	<% @secoes.each do |k,s| %>
		<h4>Seção <%=s.first.secao.codigo %></h4>
		<table class="table table-condensed table-bordered table-striped">
		  <tr>
		  	<th>Candidato</th>
				<th>Cargo</th>
				<th>Partido</th>
		    <th>Votos</th>
		  </tr>

		  <% s.each do |v| %>
			  <tr>
			    <td><%= v.candidatura.candidato.nome_campanha %></td>
			    <td><%= v.candidatura.cargo_eleicao.cargo.nome %></td>
			    <td><%= v.candidatura.partido.nome %></td>
			    <td><%= v.qtd_votos %></td>
			  </tr>
		  <% end %>
		</table>
		<br />
	<% end %>
<% end %>