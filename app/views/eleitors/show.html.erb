<div class="page-header"><h1>Dados do Eleitor</h1></div>

<p id="notice"><%= notice %></p>

<p>
  <b>Nome:</b>
  <%= @eleitor.nome %>
</p>

<p>
  <b>Mãe:</b>
  <%= @eleitor.nome_mae %>
</p>

<p>
  <b>RG:</b>
  <%= @eleitor.rg %>
</p>

<p>
  <b>Data de Nascimento:</b>
  <%= @eleitor.data_nascimento.strftime('%d/%m/%Y') %>
</p>

<p>
  <b>CPF:</b>
  <%= @eleitor.cpf %>
</p>

<p>
  <b>Endereço:</b>
  <%= @eleitor.endereco.descricao %>
</p>

<p>
  <b>Número do Título:</b>
  <%= @eleitor.titulo.codigo %>
</p>

<p>
  <b>Zona:</b>
  <%= @eleitor.titulo.secao.zona.descricao %>
</p>

<p>
  <b>Seção:</b>
  <%= @eleitor.titulo.secao.codigo %>
</p>

<p>
  <b>Data de Emissão:</b>
  <%= @eleitor.titulo.data_emissao.strftime('%d/%m/%Y') %>
</p>

<p>
  <b>Juiz Eleitoral:</b>
  <%= @eleitor.titulo.juiz_eleitoral %>
</p>


<%= link_to content_tag(:span, "Alterar"), edit_eleitor_path(@eleitor) %> |
<%= link_to content_tag(:span, "Voltar"), eleitors_path %>
