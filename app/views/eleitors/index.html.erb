<div class="page-header"><h1>Lista de eleitors</h1></div>

<table class="table table-condensed table-striped">
  <tr>
    <th>Nome</th>
    <th>Nome mae</th>
    <th>Rg</th>
    <th>Data nascimento</th>
    <th>Cpf</th>
    <th>Endereco</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @eleitors.each do |eleitor| %>
  <tr>
    <td><%= eleitor.nome %></td>
    <td><%= eleitor.nome_mae %></td>
    <td><%= eleitor.rg %></td>
    <td><%= eleitor.data_nascimento.strftime('%d/%m/%Y') %></td>
    <td><%= eleitor.cpf %></td>
    <td><%= eleitor.endereco.descricao %></td>
    <td><%= link_to content_tag(:span, "Exibir"), eleitor, :class => "btn btn-info btn-small" %></td>
    <td><%= link_to content_tag(:span, "Alterar"), edit_eleitor_path(eleitor), :class => "btn btn-info btn-small" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nova Eleitor', new_eleitor_path %>
