<%= simple_form_for @eleitor, html: {class: 'form-horizontal'} do |ele| %>
  <%= ele.error_notification %>

  <div class="form-inputs">
    
    <%= ele.input :nome %>
    <%= ele.input :nome_mae %>
    <%= ele.input :rg %>
    <%= ele.input :cpf %>
    <%= ele.input :data_nascimento, as: :date, start_year: 1900, end_year: Date.today.year %>

    <%= ele.simple_fields_for :endereco do |ende| %>
      <% render 'enderecos/endereco_fields', ende: ende %>    
    <% end %>

    <%= ele.simple_fields_for :titulos do |titu| %>
      <% render 'titulos/titulo_fields', titu: titu %>
    <% end %> 

  </div>

  <%= submit_tag 'Cancelar', type: 'button', class: "btn", onclick: "history.back();" %>
  <%= ele.button :submit %>
<% end %>