<h1>Lista de cargo_eleicaos</h1>

<table>
  <tr>
    <th>Cargo</th>
    <th>Eleicao</th>
    <th>Uf</th>
    <th>Municipio</th>
    <th>Vagas</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cargo_eleicaos.each do |cargo_eleicao| %>
  <tr>
    <td><%= cargo_eleicao.cargo.nome %></td>
    <td><%= cargo_eleicao.eleicao.ano %></td>
    <td><%= cargo_eleicao.uf.sigla if cargo_eleicao.uf %></td>
    <td><%= cargo_eleicao.municipio.nome if cargo_eleicao.municipio%></td>
    <td><%= cargo_eleicao.vagas %></td>
    <td><%= link_to content_tag(:span, "Exibir"), cargo_eleicao %></td>
    <td><%= link_to content_tag(:span, "Alterar"), edit_cargo_eleicao_path(cargo_eleicao) %></td>
    <td><%= link_to content_tag(:span, "Excluir"), cargo_eleicao, method: :delete, data: { confirm: 'Tem certeza?' } %></td>
  </tr>
<% end %>
</table>

<%= will_paginate @posts %>

<br />

<%= link_to 'Nova Cargo eleicao', new_cargo_eleicao_path %>
