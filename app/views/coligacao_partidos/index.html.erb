<div class="page-header"><h1>Partidos Coligados</h1></div>

<table class="table table-condensed table-striped">
  <tr>
    <th>Coligacao</th>
    <th>Partido</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @coligacao_partidos.each do |coligacao_partido| %>
  <tr>
    <td><%= coligacao_partido.coligacao.nome %></td>
    <td><%= coligacao_partido.partido.nome %></td>
    <td><%= link_to content_tag(:span, "Exibir"), coligacao_partido, :class => "btn btn-small" %></td>
    <td><%= link_to content_tag(:span, "Alterar"), edit_coligacao_partido_path(coligacao_partido), :class => "btn btn-small" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nova Coligacao partido', new_coligacao_partido_path, :class => "btn btn-small btn-info" %>
